<%= form_for :post, url: posts_path do |f| %>
     <p>
      <%= f.label :tittle %><br>
      <%= f.text_field :tittle %>
    </p>
     
      <p>
      <%= f.label :content %><br>
      <%= f.text_area :content %>
    </p>
     
      <p>
      <%= f.submit %>
    </p>
<% end %>

<%= link_to 'Back', posts_path %>